in page:\n\n```ts\nimport Web3 from 'web3';\nimport { Magic } from 'magic-sdk';\nimport { ConnectExtension } from '@magic-ext/connect';\n\nconst magic = new Magic('YOUR_API_KEY', {\n  extensions: [new ConnectExtension()],\n  network: testnetOrMainnet\n});\n\nconst web3 = new Web3(magic.rpcProvider);\n\nawait web3.eth.getAccounts();\n```\n","target":"browser","exports":{"import":"./dist/es/index.mjs","require":"./dist/cjs/index.js"},"gitHead":"a5c187c68d2432317081b80049c281adac0f4c0b","_npmUser":{"name":"magiclabs_fe","email":"frontend@magic.link"},"jsdelivr":"./dist/extension.js","externals":{"include":["magic-sdk"]},"deprecated":"Package no longer supported. Contact Support at https://www.npmjs.com/support for more info.","repository":{"url":"git+https://github.com/magiclabs/magic-js.git","type":"git"},"_npmVersion":"lerna/3.22.1/node@v14.17.0+x64 (linux)","description":"Magic SDK Connect Extension for Web environments.","directories":{},"_nodeVersion":"14.17.0","cdnGlobalName":"MagicConnectExtension","_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"magic-sdk":"^8.1.1"},"_npmOperationalInternal":{"tmp":"tmp/connect_0.1.2-canary.316.3098.0_1652392030649_0.6288107287637317","host":"s3://npm-registry-packages"}},"0.1.2-canary.316.3107.0":{"name":"@magic-ext/connect","version":"0.1.2-canary.316.3107.0","author":{"url":"https://magic.link/","name":"Magic","email":"team@magic.link"},"license":"MIT","_id":"@magic-ext/connect@0.1.2-canary.316.3107.0","maintainers":[{"name":"ethella","email":"ethellagit@gmail.com"},{"name":"ternstor-magic","email":"thomas@magic.link"},{"name":"damianjachyra","email":"damian@magic.link"},{"name":"mushfichowdhury","email":"mushfi@magic.link"},{"name":"magic-ravi","email":"ravi@magic.link"},{"name":"magiclabs_fe","email":"frontend@magic.link"},{"name":"jasonatmagic","email":"jason@magic.link"},{"name":"rislas","email":"rolando@magic.link"},{"name":"bcleary06","email":"brian@magic.link"},{"name":"hcote","email":"hunter@ma